# GitHub
GITHUB_TOKEN=ghp_your_token_here
AUTO_BUILDS_REPO=your-org/ga-auto-builds

# LLM Provider: openai, ollama, nvidia
LLM_PROVIDER=ollama

# OpenAI (if using openai)
OPENAI_API_KEY=sk-your-key-here
OPENAI_MODEL=gpt-4

# Ollama (if using ollama)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=codellama

# NVIDIA NIM (if using nvidia)
NVIDIA_API_KEY=nvapi-your-key-here
NVIDIA_MODEL=nvidia/llama-3.1-nem–æ-70b-instruct

# Repair Settings
MAX_ATTEMPTS=5
WAIT_INTERVAL=30
BUILD_TIMEOUT=300
BUILD_COMMAND=npm run build
MAX_TOKENS=32000
